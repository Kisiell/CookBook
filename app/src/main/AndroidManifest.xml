<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kisiel.cookbook" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListaActivity_"
            android:label="@string/title_activity_lista" >
        </activity>
        <activity
            android:name=".LoginActivity_"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".ViewRecipeActivity_"
            android:label="@string/title_activity_view_recipe" >
        </activity>
        <activity
            android:name=".AddRecipeView_"
            android:label="@string/title_activity_add_recipe_view" >
        </activity>
    </application>

</manifest>
